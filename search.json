---
layout: null
---
[
  {% for post in site.posts %}
    {
      "title": "{{ post.title | escape }}",
      "url": "{{ post.url | relative_url }}",
      "date": "{{ post.date | date: "%B %d, %Y" }}",
      "content": "{{ post.content | strip_html | strip_newlines | truncatewords: 50 | escape }}"
    }{% unless forloop.last %},{% endunless %}
  {% endfor %}
]